<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>ManageServices</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    <welcome-file>default.htm</welcome-file>
  </welcome-file-list>
  
  
  <servlet>
  	<servlet-name>ChooseUpdateSvlt</servlet-name>
  	<servlet-class>com.service.ChooseUpdateSvlt</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ChooseUpdateSvlt</servlet-name>
  	<url-pattern>/choose</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>updateService</servlet-name>
  	<servlet-class>com.service.UpdateServiceSrvlt</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>updateService</servlet-name>
  	<url-pattern>/update</url-pattern>
  </servlet-mapping>
  
  
  
  
  <servlet>
  	<servlet-name>AddServiceSvlt</servlet-name>
  	<servlet-class>com.service.AddServiceSvlt</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>AddServiceSvlt</servlet-name>
  	<url-pattern>/insert</url-pattern>
  </servlet-mapping>
  
  
  
  <servlet>
  	<servlet-name>DeleteService</servlet-name>
  	<servlet-class>com.service.DeleteServiceSrvlt</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>DeleteService</servlet-name>
  	<url-pattern>/delete</url-pattern>
  </servlet-mapping>
  
  
</web-app>